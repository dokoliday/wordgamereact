{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "baseUrl": ".",
    "rootDir": ".",
    "noUnusedLocals": true,
    "paths": {
      "@env-configs": ["env-configs/index.ts", "env-configs/index.prod.ts"]
    }
  },
  "types": "typings"
}
